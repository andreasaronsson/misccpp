####### Build rules

all: Makefile $(TARGET)

$(TARGET):  $(UICDECLS) $(OBJECTS) $(OBJMOC)  
	$(LINK) $(LFLAGS) -o $(TARGET) $(OBJECTS) $(OBJMOC) $(OBJCOMP) $(LIBS)

mocables: $(SRCMOC)
uicables: $(UICDECLS) $(UICIMPLS)

$(MOC): 
	( cd $(QTDIR)/src/moc && $(MAKE) )

Makefile: SpamMaster2000.pro  /usr/qt/3/mkspecs/linux-g++/qmake.conf 
	$(QMAKE) -o Makefile SpamMaster2000.pro
qmake: 
	@$(QMAKE) -o Makefile SpamMaster2000.pro

dist: 
	@mkdir -p .obj/SpamMaster2000 && $(COPY_FILE) --parents $(SOURCES) $(HEADERS) $(FORMS) $(DIST) .obj/SpamMaster2000/ && $(COPY_FILE) --parents images/editcopy images/editcut images/editpaste images/filenew images/fileopen images/filesave images/print images/redo images/redstar.png images/searchfind images/stalin_klar.jpg images/undo images/stalin_2.jpg .obj/SpamMaster2000/ && $(COPY_FILE) --parents abookform.ui.h addcontact.ui.h mainWindow.ui.h newmailboxdialog.ui.h savemail.ui.h sendmailform.ui.h .obj/SpamMaster2000/ && ( cd `dirname .obj/SpamMaster2000` && $(TAR) SpamMaster2000.tar SpamMaster2000 && $(GZIP) SpamMaster2000.tar ) && $(MOVE) `dirname .obj/SpamMaster2000`/SpamMaster2000.tar.gz . && $(DEL_FILE) -r .obj/SpamMaster2000

